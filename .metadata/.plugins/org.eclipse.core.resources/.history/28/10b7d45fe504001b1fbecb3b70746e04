package br.com.universidade.modelo;

public class Bacharelado extends Formacao {
	private int tipo;
	//construtores

	public Bacharelado(float valor, String descricao, int duracao, int tipo) {
		super(valor, descricao, duracao);
		this.tipo = tipo;
	}
	public Bacharelado() {
		super();
	}
	
		
	//get all e set all
	
	public String getAll() {
		return
				tipo + "\n" +
				super.getAll();
	}
	public void setAll(float valor, String descricao, int duracao, int tipo) {
		super.setAll(valor, descricao, duracao);
		this.tipo = tipo;
	}
	
	//gets e sets individuais
	
	public int getTipo() {
		return tipo;
	}
	public void setTipo(int tipo) {
		this.tipo = tipo;
	}
}
