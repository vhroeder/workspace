package br.com.universidade.modelo;

public class Pos extends Formacao {
	private String projetoConclusao;
	private int cargaHorariaEstagio;
	//construtores

	public Pos(float valor, String descricao, int duracao, String projetoConclusao, int cargaHorariaEstagio) {
		super(valor, descricao, duracao);
		this.projetoConclusao = projetoConclusao;
		this.cargaHorariaEstagio = cargaHorariaEstagio;
	}
	public Pos() {
		super();
	}
		
	//get all e set all
	
	public String getAll() {
		return
				"Projeto de conclusão: " + this.projetoConclusao + "\n" +
				"Carga Horária de Estágio: " + this.cargaHorariaEstagio + "\n" +
				super.getAll();
	}
	public void setAll(float valor, String descricao, int duracao, String projetoConclusao, int cargaHorariaEstagio) {
		super.setAll(valor, descricao, duracao);
		this.projetoConclusao = projetoConclusao;
		this.cargaHorariaEstagio = cargaHorariaEstagio;
	}
		
	//gets e sets individuais
	
	public String getProjetoConclusao() {
		return projetoConclusao;
	}
	public void setProjetoConclusao(String projetoConclusao) {
		this.projetoConclusao = projetoConclusao;
	}
	public int getCargaHorariaEstagio() {
		return cargaHorariaEstagio;
	}
	public void setCargaHorariaEstagio(int cargaHorariaEstagio) {
		this.cargaHorariaEstagio = cargaHorariaEstagio;
	}

}
	