package br.com.lojauati.teste;

import br.com.lojauati.modelo.Formula1;

public class TesteFormula1 {

	public static void main(String[] args) {
		//Formula1 objeto; //criando um objeto
		//objeto.preencherEscuderia("ferrari"); NÃO FUNCIONA
		
		Formula1 carro = new Formula1(); //instanciando um objeto
		carro.preencherEscuderia("ferrari");
		System.out.println(carro.exibirEscuderia());
		carro.calcularValor("America", 2000);
		System.out.println(carro.exibirValor());
		carro.calcularRpm(8, 1000);
		System.out.println(carro.exibirRpm());
	}

}
